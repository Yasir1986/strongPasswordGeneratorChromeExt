{
  "manifest_version": 3,
  "name": "Strong Password Generator",
  "version": "1.1",
  "description": "Generate strong, secure passwords locally.",
   "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}